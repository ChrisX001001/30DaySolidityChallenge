pragma solidity >=0.5.0 < 0.9.0;

contract callfunction{
    uint8 a = 10;
    function calla() public view returns(uint8){
        return a;
    }
   // string str= "solidity is easy";
   function callstr() public view returns(uint8){
       uint8 str= 89;
       return str;
   } 
} 
